EXTRA_CFLAGS=\
-D__KERNEL__=1 \
-DENABLE_ASSERT=@ENABLE_ASSERT@ \
-DENABLE_DEBUG=@ENABLE_DEBUG@ \
-DDRV_NAME=\"@CCIV4DRV_DRV_NAME@\" \
-DDRV_VERSION=\"@CCIV4DRV_VERSION@\" \
-I$(M) \
-I$(aalkernelinc)

obj-m                  :=@CCIV4DRV_DRV_NAME@.o
@CCIV4DRV_DRV_NAME@-objs:= cciv4_driver_main.o cciv4_simulator.o cciv4_sim_afu_pip.o cciv4_PIPsession.o cciv4_common.o cciv4_sim_afu_mmap.o cciv4_sim_mafu_pip.o kOSAL.o
